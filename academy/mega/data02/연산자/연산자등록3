package 연산자;

import javax.swing.JOptionPane;

public class 연산자등록3 {

	public static void main(String[] args) {
		// 총 가격을 입력 받으세요.
		String tprice = JOptionPane.showInputDialog("총 가격");
		int tprice2 = Integer.parseInt(tprice);
		// int tprice2 = Integer.parseInt(JOptionPane.showInputDialog("총 가격"));
		
		JOptionPane.showMessageDialog(null, tprice2);
		
		// 반복문 : 초기값, 조건식, 증감식
		// 0~9까지
		for (int i = 0; i < 10; i++)  {
			System.out.println(i);
		// Syntax error(문법 오류)
		// debug 이용시
		// <- 숫자 앞에 더블 클릭 : breakpoint 지점에서 멈춘다. resume으로 한번씩 진행
		} 
		
		System.out.println("여기가 끝!");
		
	}

}
