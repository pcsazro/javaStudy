package 변수;

public class 자바 {
	//main + ctrl+spacebar (생성시 메인으로 만들지 않았을때)
	public static void main(String[] args) {
		// 자동완성 ctrl+spacebar
		// sysout, syso + ctrl + spacebar
		// 자바 프로그램으로 나는 어떤 난이도의 코딩을 하고 싶은가?
		// 자바 프로그램 중 어떤 프로그램을 해보고 싶은가?
		// 5줄 이상.
		System.out.println("중상급의 코딩을 어떤상태에서도 만들수 있는 기본기를 원한다.");
		System.out.println("웹을 이용한 프로그램이나, 상업용 프로그램, 데이터 수집 프로그램을 만들어 보고 싶다.");

	}

}
